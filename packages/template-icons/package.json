{
  "name": "@codesandbox/template-icons",
  "version": "0.5.0",
  "description": "Collection of SVG icons as React Components for usage with CodeSandbox Templates.",
  "keywords": [
    "codesandbox",
    "react"
  ],
  "license": "MIT",
  "author": "Drake Costa <drake@codesandbox.io> (https://github.com/Saeris)",
  "contributors": [
    "Ives van Hoorne <ives@codesandbox.io> (https://github.com/CompuIves)",
    "Sara Vieira <sara@codesandbox.io> (https://github.com/SaraVieira)"
  ],
  "homepage": "https://codesandbox.io/",
  "repository": "https://github.com/codesandbox/codesandbox-templates/tree/master/packages/template-icons",
  "bugs": "https://github.com/codesandbox/codesandbox-templates/issues",
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/codesandbox",
    "logo": "https://opencollective.com/codesandbox/logo.txt"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/index.ts",
  "scripts": {
    "build:example": "react-scripts build",
    "build:pkg": "pack build",
    "deploy": "run-s build:pkg build:example",
    "dev": "nodemon --watch src --watch example --exec run-s build:pkg start",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start": "react-scripts start",
    "test": "yarn lint && jest -c ./jest.config.js",
    "test:watch": "jest -c ./jest.config.js --watch",
    "test:coverage": "jest -c ./jest.config.js && codecov",
    "release": "pack publish",
    "version": "yarn build:pkg"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.2",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@pika/pack": "^0.4.0",
    "@pika/plugin-build-types": "^0.4.0",
    "@pika/plugin-build-web": "^0.4.0",
    "@pika/plugin-standard-pkg": "^0.4.0",
    "@testing-library/react": "^8.0.4",
    "@types/react": "16.8.23",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "codecov": "^3.5.0",
    "core-js": "3.1.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "~7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "nodemon": "1.19.1",
    "npm-run-all": "^4.1.5",
    "opencollective": "^1.0.3",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__TEST__/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-types"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
